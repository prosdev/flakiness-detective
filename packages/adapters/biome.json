{
  "$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
  "extends": [
    "../../.biome.json"
  ],
  "linter": {
    "enabled": true,
    "rules": {
      "suspicious": {
        "noExplicitAny": "off"
      },
      "complexity": {
        "noForEach": "off",
        "noBannedTypes": "off"
      },
      "correctness": {
        "noUnusedVariables": "off"
      }
    }
  }
}